
# 12. STUDENT_LOGIN (Autenticación de estudiante por Matrícula/Contraseña)
gcloud functions deploy studentLogin --runtime nodejs20 --trigger-http --entry-point studentLogin --source . --region us-central1 --allow-unauthenticated --env-vars-file .env.yaml

